<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_edit_External_ID</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
!ISNEW(),
ISCHANGED( ExternalId__c )
)</errorConditionFormula>
    <errorMessage>External ID can not be edited.</errorMessage>
</ValidationRule>
