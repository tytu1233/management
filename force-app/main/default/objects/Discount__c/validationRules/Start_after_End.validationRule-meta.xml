<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_after_End</fullName>
    <active>true</active>
    <errorConditionFormula>Start_Date__c &gt; End_Date__c</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Start Date should be picked before End Date</errorMessage>
</ValidationRule>
